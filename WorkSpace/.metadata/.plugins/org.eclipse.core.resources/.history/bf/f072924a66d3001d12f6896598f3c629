package pack03_for;

import java.util.Scanner;

public class Test01_For {
	public static void main(String[] args) {
		//1. 별찍기
		for(int s=1; s<=5; s++) {
			for(int t=1; t<=s; t++) {
				System.out.print("t");
			}
			System.out.println();
			}
		//2. 구구단
	/*	Scanner sc = new Scanner(System.in);

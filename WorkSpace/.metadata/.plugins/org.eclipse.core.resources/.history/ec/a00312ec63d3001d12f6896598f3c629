package pack03_for;

import java.util.Scanner;

public class Test01_For {
	public static void main(String[] args) {
		//1. 별찍기
		for(int s=1; s<6; s++) {
			System.out.println(s);
		}
		System.out.println("============");
		//2. 구구단
		Scanner sc = new Scanner(System.in);
		String data1 = sc.nextLine();
		System.out.println(data1);
	}//m
}//c

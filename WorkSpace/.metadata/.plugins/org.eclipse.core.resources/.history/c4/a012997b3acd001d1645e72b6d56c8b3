package pack01_variable;

public class Ex03_variable {
	public static void main(String[] args) {

		int value = 10;
		int result = value + 10;
		System.out.println(result);
//¿Á«“¥Á
		result=1+1+10;
		System.out.println(result);
		result=10-1+5;
		System.out.println(result);
		result=2*5*10;
		System.out.println(result);
		result=20/10;
		System.out.println(result);
	}
}

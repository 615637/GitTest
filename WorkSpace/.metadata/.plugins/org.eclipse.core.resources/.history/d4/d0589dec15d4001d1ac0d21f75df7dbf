package pack04_while;

public class Ex01_While {
public static void main(String[] args) {
	//for 구조... for(반복에 사용할 변수 초기화 ; 조건식 ; 증감){
	//반복할 코드}
	
	//while : 1.변수 초기화
	// while (2.조건식){
	// 반복구간
	// 3.증감식 또는 break로 반복 종료
	// }
	int i=0;
	while(i<10) {
		System.out.print(i);
		i++;
	}
//	int i=1;
//	while(i<=50) {
}
}

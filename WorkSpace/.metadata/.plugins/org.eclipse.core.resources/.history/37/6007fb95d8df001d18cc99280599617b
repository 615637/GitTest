package japangi001;

public class JapangiDTO001 {
	int Price, cnt;
	String bev;
	
	public JapangiDTO001(int price, int cnt, String bev) {
		this.Price = price;
		this.cnt = cnt;
		this.bev = bev;
	}

	public int getPrice() {
		return Price;
	}

	public void setPrice(int price) {
		Price = price;
	}

	public int getCnt() {
		return cnt;
	}

	public void setCnt(int cnt) {
		this.cnt = cnt;
	}

	public String getBev() {
		return bev;
	}

	public void setBev(String bev) {
		this.bev = bev;
	}
	
	

	
}

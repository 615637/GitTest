package pack02.inter;

public class Audio implements RemoteControl {
	private int volume;
	@Override
	public void turnOn() {
		System.out.println("Audio에 전원을 켭니다.");
	}

	@Override
	public void turnOff() {
		System.out.println("Audio에 전원을 끕니다.");
	}

	@Override
	public void setVolume(int volume) {
		
		
	}
	
}
